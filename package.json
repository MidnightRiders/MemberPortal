{
  "name": "midnight-riders-member-portal",
  "version": "1.0.0",
  "description": "Member Portal for Midnight Riders; migrating from HAML to Preact",
  "license": "MIT",
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds --minify --public-path=assets",
    "build:css": "sass ./app/assets/stylesheets/entrypoints:./app/assets/builds --style=compressed --load-path=node_modules",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@types/jquery": "^3.5.14",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.11.18",
    "@types/postcss-import": "^14.0.0",
    "@types/postcss-mixins": "^9.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "postcss-import": "^15.1.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.0",
    "postcss-variables": "^1.1.1",
    "prettier": "^2.8.0",
    "rollup-plugin-gzip": "^3.1.0",
    "rollup-plugin-strip-prop-types": "^1.0.3",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-full-reload": "^1.0.5",
    "vite-plugin-ruby": "^3.1.2"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@mui/material": "^5.11.2",
    "@preact/signals": "^1.1.3",
    "@sentry/browser": "^7.28.1",
    "@stripe/react-stripe-js": "^1.16.1",
    "@stripe/stripe-js": "^1.46.0",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "esbuild": "^0.16.4",
    "jquery-ujs": "^1.2.3",
    "js-cookie": "^3.0.1",
    "logrocket": "^3.0.1",
    "preact": "^10.11.3",
    "sass": "^1.56.2",
    "wouter-preact": "^2.9.0"
  },
  "type": "module"
}
