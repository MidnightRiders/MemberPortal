    "format:es": "f() { eslint --fix --format unix ${1:-app/javascript}; }; f",
    "lint": "npm-run-all --parallel --continue-on-error --aggregate-output lint:es lint:css",
    "lint:css": "f() { stylelint --formatter unix ${1:-'app/javascript/**/*.css'}; }; f",
    "lint:es": "f() { eslint --format unix ${1:-app/javascript}; }; f",
    "typecheck": "tsc --noEmit"
